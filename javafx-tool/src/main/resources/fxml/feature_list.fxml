<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns:fx="http://javafx.com/fxml" cache="true" prefHeight="39.0"
  stylesheets="@../skin/bootstrap3.css"
  xmlns="http://javafx.com/javafx"
  fx:controller="zjtech.modules.home.FeatureListController">
  <padding>
    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
  </padding>

  <Button mnemonicParsing="false" onAction="#click" text="%feature.cartoon" fx:id="picBtn">
    <styleClass>
      <String fx:value="default"/>
      <String fx:value="first"/>
    </styleClass>
  </Button>
  <Button mnemonicParsing="false" onAction="#click" text="Web" fx:id="middleBtn">
    <styleClass>
      <String fx:value="default"/>
      <String fx:value="middle"/>
    </styleClass>
  </Button>
  <Button mnemonicParsing="false" text="Other">
    <styleClass>
      <String fx:value="default"/>
      <String fx:value="last"/>
    </styleClass>
  </Button>
</HBox>
