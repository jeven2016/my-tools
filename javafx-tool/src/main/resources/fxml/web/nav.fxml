<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml"
  prefHeight="160.0"
  prefWidth="100.0"
  stylesheets="@../../skin/bootstrap3.css"
  xmlns="http://javafx.com/javafx" fx:controller="zjtech.web.WebNavController">
  <fx:define>
    <ToggleGroup fx:id="webMyToggleGroup"/>
  </fx:define>
  <VBox alignment="TOP_CENTER">
    <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
    <ToggleButton mnemonicParsing="false" onAction="#showContent" prefWidth="150"
      text="Websocket Client"
      toggleGroup="$webMyToggleGroup"
      fx:id="wsClientBtn">
      <styleClass>
        <String fx:value="first"/>
        <String fx:value="action-btn"/>
      </styleClass>
      <VBox.margin>
        <Insets bottom="-1.0"/>
      </VBox.margin>
    </ToggleButton>
  </VBox>
</AnchorPane>
