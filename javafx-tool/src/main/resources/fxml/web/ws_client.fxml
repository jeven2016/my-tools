<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>

<TitledPane styleClass="info" stylesheets="@../../skin/bootstrap3.css" text="WebSocket Client Test" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="zjtech.web.WsClientController">

    <AnchorPane prefHeight="500.0" prefWidth="833.0">
        <children>
            <ListView layoutX="14.0" layoutY="61.0" prefHeight="282.0" prefWidth="545.0" />
            <TextField layoutX="14.0" layoutY="14.0" prefWidth="284.0" />
            <Button fx:id="connectBtn" layoutX="310.0" layoutY="14.0" mnemonicParsing="false" onAction="#connect" text="Connect">
                <styleClass>
                    <String fx:value="success" />
                    <String fx:value="action-btn" />
                </styleClass>
            </Button>

            <Button fx:id="clearBtn" layoutX="20.0" layoutY="69.0" mnemonicParsing="false" onAction="#clear" text="Clear">
                <styleClass>
                    <String fx:value="btn-default" />
                    <String fx:value="action-btn" />
                </styleClass>
            </Button>


            <TitledPane layoutX="11.0" layoutY="368.0" prefHeight="248.0" prefWidth="549.0" styleClass="info" text="Message" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
                <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="212.0" prefWidth="547.0">
                        <children>
                            <TextArea layoutX="14.0" layoutY="14.0" prefHeight="145.0" prefWidth="524.0" />
                            <Button fx:id="sendBtn" layoutX="218.0" layoutY="163.0" mnemonicParsing="false" onAction="#send" text="Send">
                                <styleClass>
                                    <String fx:value="success" />
                                    <String fx:value="action-btn" />
                                </styleClass>
                            </Button>
                        </children>
                    </AnchorPane>
                </content>
            </TitledPane>


        </children>
    </AnchorPane>
</TitledPane>
